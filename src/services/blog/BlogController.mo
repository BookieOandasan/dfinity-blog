import Array "mo:base/Array";
import Char "mo:base/Char";
import Debug "mo:base/Debug";
import ExperimentalCycles "mo:base/ExperimentalCycles";
import Hash "mo:base/Hash";
import HashMap "mo:base/HashMap";
import Nat "mo:base/Nat";
import Option "mo:base/Option";
import Principal "mo:base/Principal";
import PublicTypes "../public_types";
import Text "mo:base/Text";
import Time "mo:base/Time";
import Types "../types";

module {
    public class BlogController() {
        var blogId : Nat = 0;
        var pageId : Nat = 0;
 
        let postDb = HashMap.HashMap<Nat, Types.BlogPost>(0, isIntEq, Hash.hash);
        let pageDb = HashMap.HashMap<Nat, Types.Page>(0, isIntEq, Hash.hash);

        // PubSub (✿◠‿◠)
        var registrySubscribers : [PublicTypes.BlogPostPubMessageCallback] = [];

        var blogMeta : Types.BlogMeta = {   
            image = ?"data:image/jpeg;base64,";
            links = [
                // Uncomment once FE handles {id = 1; text = "Small Link"}
            ];
            name = "Suddenly Hazel!";
            summary = ?"Wow this is my blog";
            tags = ["New Blog"];
        };

        // Meta funcs
        public func getMeta() : Types.BlogMeta {
            return blogMeta;
        };

        public func updateMeta(newMeta : Types.BlogMeta) {
            blogMeta := newMeta;
        };

        public func registerBlog(registry : Principal) : async Bool {
            let reg : PublicTypes.RegistryCanister = actor(Principal.toText(registry));
            switch(await reg.registerBlog()) {
                case (#err(v)) {false};
                case (#ok(null)) {
                    true
                };
                case (#ok(?cb)) {
                    registrySubscribers := Array.append(registrySubscribers, [cb]);
                    true;
                }
            };
        };

        // Page funcs
        public func createPage(page: Types.Page) : Bool {
            pageId := pageId + 1;
            pageDb.put(pageId, page);
            return true;
        };

        public func getPage(id : Nat ) : ?Types.Page {
            return pageDb.get(id);
        };

        public func deletePage(id : Nat) {
            pageDb.delete(id);
        };

        public func listPages() : [Types.PageSummary] {
            var pageSum : [Types.PageSummary] = [];
            for ((k, v) in pageDb.entries()) {
                pageSum := Array.append(pageSum, [{
                    title = v.title;
                    id = k;
                }])
            };
            return pageSum;
        };

        // Post funcs
        public func createPost(egg : Types.BlogPostEgg) : Types.BlogPostPubMessage {
            blogId := blogId + 1;
            let now = Time.now();
            postDb.put(blogId, {
                socialImage = egg.socialImage;
                content = egg.content;
                title = egg.title;
                time = now;
                tags = egg.tags;
            });
            return {
                id = blogId;
                title = egg.title;
                time = now;
            };
        };

        public func notifySubscribers(msg : Types.BlogPostPubMessage) : async () {
            for (cb in Array.vals(registrySubscribers)) {
                let _ = cb(msg);
            };
        };

        public func getPost(id : Nat) : ?Types.BlogPost{
            return postDb.get(id);
        };

        // TODO paginate {limit: Nat, offset: Nat} -> {hasMore: Bool, posts: [Posts..]}
        // TODO Blarg.. need to order on ID here..
        public func listPosts() : [Types.BlogPostSummary] {
            var posts : [Types.BlogPostSummary] = [];

            for ((k, v) in postDb.entries()) {
                // TODO this should be precalculated
                let thisSum : Text =  do {
                        var text = "";
                        label a for (c in Text.toIter(v.content)){
                            text := text # Char.toText(c);
                            if (Text.size(text) > 100) {
                                 break a;
                            };
                        };
                        text;
                };

                posts := Array.append(posts, [{
                    socialImage = v.socialImage;
                    id = k;
                    title = v.title;
                    time = v.time;
                    summary = thisSum;
                }]);
            };

            return posts;
        };

        // Generate some seed data
        let init = do {
            let a = createPost({
                socialImage = null;
                content = "Wow this is a test post!";
                title = "Demo Post #1";
                tags = ["Demo"];
            });

            let b = createPost({
                socialImage = null;
                content = "Wow another test post!";
                title = "Demo Post #2";
                tags = ["Demo"];
            });

            let c = createPage({
                title = "Sample Page";
                content = "# Just a Sample Page\n\nHey wow!"
            });

            blogMeta := {
                image = blogMeta.image;
                links = [{id = pageId; text = "Demo Page";}];
                name = blogMeta.name;
                summary = blogMeta.summary;
                tags = blogMeta.tags;
            };
        };
    };

    private func isIntEq(a : Nat, b : Nat) : Bool {
        return a == b;
    }
}